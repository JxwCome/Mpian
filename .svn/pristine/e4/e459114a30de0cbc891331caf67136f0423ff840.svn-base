.about-container {
  width: 690rpx;
  margin: 0 auto;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.about-container image {
  width: 152rpx;
  height: 152rpx;
  border-radius: 50%;
  margin-top: 104rpx;
}
.about-container .edition {
  font-size: 28rpx;
  margin: 44rpx 0 72rpx;
  color: #9a9a9a;
}
.about-container .intro {
  line-height: 48rpx;
}
.about-container .public {
  margin-top: 560rpx;
  font-size: 28rpx;
  text-align: center;
  line-height: 40rpx;
}