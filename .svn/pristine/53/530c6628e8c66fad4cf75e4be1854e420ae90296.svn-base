.people-sex{
  width: 100%;
  height: 180rpx;
  padding-top: 80rpx;
  text-align: center;
  border-bottom: 1rpx solid #DDD;
}
.sex-icon{
  width: 80rpx;
  height: 80rpx;
}
.sex-text-man,.sex-text-woman{
  font-size: 28rpx;
  color: #09BB07;
  margin-top: 20rpx;
  display: block;
}
.sex-text-woman{
  color: #e95098;
}
.people-label{
  padding: 28rpx 30rpx;
  overflow: hidden;
}
.label-type{
  background: #68C39F;
  font-size: 28rpx;
  color: #fff;
  padding: 20rpx 25rpx;
  float: left;
  border-radius: 8rpx;
  margin: 0 20rpx 20rpx 0;
}
