.people-sex{
  width: 100%;
  height: 180rpx;
  padding-top: 80rpx;
  text-align: center;
  border-bottom: 1rpx solid #DDD;
}
.sex-icon{
  width: 80rpx;
  height: 80rpx;
}
.sex-text{
  font-size: 28rpx;
  color: #09BB07;
  margin-top: 30rpx;
  display: block;
}
.people-label{
  padding: 28rpx 30rpx;
  overflow: hidden;
}
.label-type{
  background: #68C39F;
  font-size: 28rpx;
  color: #fff;
  padding: 20rpx 25rpx;
  float: left;
  border-radius: 8rpx;
  margin: 0 20rpx 20rpx 0;
}
.sensation{
  padding: 40rpx 30rpx;
}
.label{
  overflow: hidden;
  display: inline-block;
  margin: 0 20rpx 20rpx 0;
}
.label-text{
  float: left;
  font-size: 28rpx;
  color: #fff;
  padding: 12rpx;
  background: #68C49F;
  border-bottom-left-radius: 8rpx;
  border-top-left-radius: 8rpx;
}
.label-num{
  float: left;
  padding: 12rpx;
  background: #209C82;
  font-size: 28rpx;
  color: #fff;
  border-top-right-radius: 8rpx;
  border-bottom-right-radius: 8rpx;
}
.click-view{
  margin-top: 40rpx;
  text-align: center;
  font-size: 26rpx;
  color: #999;
}