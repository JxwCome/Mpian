.people-sex{
  width: 100%;
  height: 180rpx;
  padding-top: 80rpx;
  text-align: center;
  border-bottom: 1rpx solid #DDD;
}
.sex-icon{
  width: 80rpx;
  height: 80rpx;
}
.sex-text-man,.sex-text-woman{
  color: #00a0e9;
  margin-top: 20rpx;
  display: block;
}
.sex-text-woman{
  color: #e95098;
}
.people-label{
  padding: 28rpx 30rpx;
  overflow: hidden;
}
label{
  position: relative;
  display: inline-block;
}
.label-typed,.label-type{
  background: #68C39F;
  font-size: 28rpx;
  color: #fff;
  height: 56rpx;
  line-height: 56rpx;
  padding: 0 25rpx;
  float: left;
  border-radius: 8rpx;
  margin: 0 20rpx 20rpx 0;
}
.sensation{
  padding: 60rpx 30rpx;
  overflow: hidden;
}
.label{
  float: left;
  margin: 0 20rpx 20rpx 0;
  color: #fff;
}
.label-text{
  float: left;
  padding: 0 12rpx;
  background: #68C49F;
  border-bottom-left-radius: 8rpx;
  border-top-left-radius: 8rpx;
  height: 56rpx;
  line-height: 56rpx;

}
.label-num{
  float: left;
  padding: 0 12rpx;
  background: #209C82;
  border-top-right-radius: 8rpx;
  border-bottom-right-radius: 8rpx;
  height: 56rpx;
  line-height: 56rpx;
}
.label-typed{
  background: #209C82;
}
.check{
  position: absolute;
  top: 0;
  left: 0;
}
.click-view{
  text-align: center;
  color: #999;
  font-size: 26rpx;
}
.share-btn  {
  margin-top: 58rpx;
}