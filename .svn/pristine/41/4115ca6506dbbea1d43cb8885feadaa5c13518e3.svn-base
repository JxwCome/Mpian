var app = getApp();
var constract = require('../../utils/constract.js');
Page({
  data: {
    flag:true,
    collect: []
  },
  onLoad:function (options){
    var that = this;
    console.log(options);
    var type=options.type;
    if(type==1){
      wx.setNavigationBarTitle({
        title: '点赞我的'
      });
      app.Verification(function (token) {
        wx.request({
          url: constract.findLikeme,
          header: {token:token},
          success: function (res) {
            console.log(res);
            that.setData({
              collect: res.data.data,
              flag:false
            })
          }
        })
      })
      
    };
    if (type == 2) {
      wx.setNavigationBarTitle({
        title: '收藏我的'
      });
      app.Verification(function (token) {
        wx.request({
          url: constract.findCollectme,
          header: { token: token },
          success: function (res) {
            console.log(res);
            that.setData({
              collect: res.data.data,
              flag:true
            })
          }
        })
      })
    }
    
  }
  
})