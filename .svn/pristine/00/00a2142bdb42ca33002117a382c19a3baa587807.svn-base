@import "/template/tem-impress.wxss";
/*登陆未注册*/
.site-user{
  background: #EFEFF4;
  height: 360rpx;
  width: 100%;
  overflow: hidden;
}
.user-message{
  background: #fff;
  height: 280rpx;
  margin: 10rpx;
  text-align: center;
  padding-top: 60rpx;
}
.mes-logo{
  width: 120rpx;
  height: 120rpx;
  border-radius: 10rpx;
}
.mes-userName{
  margin-top: 30rpx;
  font-size: 38rpx;
  color: #000;
}
.site-material{
  text-align: center;
}
.material-img{
  margin-top: 140rpx;
  width: 100%;
}
.material-img image{
  width: 80rpx;
  height: 104rpx;
}
.material-text{
  margin-top: 60rpx;
  font-size: 28rpx;
  color: #999;
}
.material-btn{
  width: 690rpx;
  margin-top: 90rpx;
  background: #09BB07;
  color: #fff;
}
.site-impress{
  width: 100%;
  border-bottom: 6rpx solid #f2f2f2;
}
.site-module{
  display: none;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6)
}
.mod-img{
  position: absolute;
  right: 70rpx;
  top: 0;
  width: 86rpx;
  height: 79rpx;
}
.mod-text{
  color: #fff;
  position: absolute;
  top: 100rpx;
  right: 40rpx;
}
.mod-btn{
  position: absolute;
  width: 690rpx;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
.show{
  display: block;
}
.site-share {
  margin-bottom: 30rpx;
}