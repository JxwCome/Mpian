.authentication-tit {
  color: #999;
  font-size: 26rpx;
  margin: 48rpx 0;
}
.authentication-body {
  width: 650rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.upload-item-box {
  width: 300rpx;
  margin-bottom: 86rpx;
}
.level {
  font-size: 26rpx;
  text-align: center
}
.upload-item {
  width: 300rpx;
  height: 300rpx;
  display: flex;
  box-sizing: border-box;
  border-radius: 10rpx;
  background-color: #f7f7f8;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  padding: 54rpx;
  margin-bottom: 24rpx;
}
.mingpian {
  width: 140rpx;
  height: 140rpx;
}
.authentication-ps {
  font-size: 26rpx;
  color: #999;
}
.upload-container {
  width: 690rpx;
  margin: 0 auto;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.upload-container text {
  font-size: 26rpx;
  color: #999;
}
.upload-container image {
  width: 300rpx;
  height: 300rpx;
  border-radius: 10rpx;
  margin: 164rpx 0 66rpx;
}
.share-btn {
  margin-top:92rpx; 
}

.success-container {
  width: 690rpx;
  margin: 0 auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 26rpx;
  color: #999;
}
 .success-tit {
  margin-top: 76rpx;

}
.success-container image {
  margin: 66rpx 0 50rpx;
  width: 300rpx;
  height: 300rpx;
}
.success-container .ps-tit {
  margin-bottom: 22rpx;
}
.ps-info {
  color: #333;
}