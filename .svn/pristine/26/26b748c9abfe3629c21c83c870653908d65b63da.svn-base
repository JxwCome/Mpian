.sensation{
  padding: 40rpx 30rpx;
}
.label{
  display: inline-block;
  overflow: hidden;
  margin: 0 20rpx 20rpx 0;
}
.label-text{
  float: left;
  font-size: 28rpx;
  color: #fff;
  padding: 12rpx;
  background: #68C49F;
  border-bottom-left-radius: 8rpx;
  border-top-left-radius: 8rpx;
}
.label-num{
  float: left;
  padding: 12rpx;
  background: #209C82;
  font-size: 28rpx;
  color: #fff;
  border-top-right-radius: 8rpx;
  border-bottom-right-radius: 8rpx;
}
.click-view{
  margin-top: 40rpx;
  text-align: center;
  font-size: 26rpx;
  color: #999;
}