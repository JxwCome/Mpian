<!--her-impress.wxml-->
<view class="sensation">
  <view class="label" data-id="{{item.id}}" data-name="{{item.impression_name}}"  wx:for = "{{labelElse}}" bindtap="spot">
    <text class="label-text">{{item.impression_name}}</text>
    <text class="label-num">{{item.num}}</text>
  </view>
  <view class="click-view">点击印象,评价您心中的好友印象</view>
  <view class="new-impress">
    <navigator type="primary" url="../../index/impress/new-impress/new-impress" class="new-impress-btn">添加新的印象</navigator>
    <view class="new-impress-text">上面的印象不合适? 添加一个试试</view>
  </view>
</view>
